#ifndef SWIFI_H_
#define SWIFI_H_

enum SWIFI_CMD {
    SWIFI_TEXT = 0,
    SWIFI_STACK,
    SWIFI_HEAP,
    SWIFI_DATA,
    SWIFI_BSS,
    SWIFI_BRANCH,
    SWIFI_INVERSE,
    SWIFI_PTR,
    SWIFI_DSTSRC,
    SWIFI_INTERFACE,
    SWIFI_INIT,
    SWIFI_TEST_TEXT,
    SWIFI_TEST_STACK,
    SWIFI_TEST_HEAP,
    SWIFI_TEST_DATA,
    SWIFI_TEST_BSS,
    SWIFI_TEST_BRANCH,
    SWIFI_TEST_INVERSE,
    SWIFI_TEST_PTR,
    SWIFI_TEST_DSTSRC,
    SWIFI_TEST_INTERFACE,
    SWIFI_TEST_INIT,
